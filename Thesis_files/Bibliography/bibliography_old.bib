@article{meskarpouramiriSystematicReviewFactors2021,
  title = {Systematic Review of Factors Determining Health Care Expenditures},
  author = {Meskarpour Amiri, Mohammad and Kazemian, Mahmood and Motaghed, Zahra and Abdi, Zhaleh},
