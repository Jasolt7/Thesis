@article{telloAirTrafficController2018,
  title = {The {{Air Traffic Controller Work-Shift Scheduling Problem}} in {{Spain}} from a {{Multiobjective Perspective}}: {{A Metaheuristic}} and {{Regular Expression-Based Approach}}},
  shorttitle = {The {{Air Traffic Controller Work-Shift Scheduling Problem}} in {{Spain}} from a {{Multiobjective Perspective}}},
  author = {Tello, Faustino and Mateos, Alfonso and {Jim{\'e}nez-Mart{\'i}n}, Antonio and Su{\'a}rez, Ad{\'a}n},
  year = {2018},
  journal = {Mathematical Problems in Engineering},
